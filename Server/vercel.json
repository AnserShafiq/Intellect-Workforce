{
    "version":2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/index.js"
        },{
          "src": "/contact-us-form",
          "methods" : ["POST"],
          "dest": "/index.js"
        }
    ]
  }